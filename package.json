{
  "name": "network-monitoring-dashboard",
  "version": "1.0.0",
  "description": "AI-powered Network Monitoring Analytics Dashboard with RAG and SQL capabilities",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "analyze": "node scripts/analyzeData.js",
    "setup": "node scripts/setupDatabase.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "keywords": [
    "network-monitoring",
    "analytics",
    "ai",
    "rag",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
